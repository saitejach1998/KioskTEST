<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.saiteja.kiosktest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".facultyActivity"
            android:label="@string/title_activity_faculty"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.saiteja.kiosktest.MainActivity" />
        </activity>
        <activity
            android:name=".LabActivity"
            android:label="@string/title_activity_lab"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.saiteja.kiosktest.MainActivity" />
        </activity>
        <activity
            android:name=".classActivity"
            android:label="@string/title_activity_class"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.saiteja.kiosktest.MainActivity" />
        </activity>
        <activity
            android:name=".navActivity"
            android:label="@string/title_activity_nav"
            android:parentActivityName=".MainActivity" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.saiteja.kiosktest.MainActivity" />

        <activity
            android:name=".aboutActivity"
            android:label="@string/title_activity_about"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.saiteja.kiosktest.MainActivity" />
        </activity>
        <activity
            android:name=".faculty_profile"
            android:label="@string/title_activity_faculty_profile"
            android:parentActivityName=".facultyActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.saiteja.kiosktest.facultyActivity" />
        </activity>
    </application>

</manifest>